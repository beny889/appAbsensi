{
  "name": "absensi-web-admin",
  "version": "1.0.0",
  "description": "Web Admin Panel for Absensi System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "copy .env.local .env && vite",
    "dev:production": "copy .env.production .env && vite",
    "build:local": "copy .env.local .env && tsc && vite build",
    "build:production": "copy .env.production .env && tsc && vite build",
    "dev:testing": "copy .env.testing .env && vite",
    "build:testing": "copy .env.testing .env && tsc && vite build",
